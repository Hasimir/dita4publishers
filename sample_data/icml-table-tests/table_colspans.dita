<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA Topic//EN" "topic.dtd">
<topic id="topic_dmp_xwd_gm">
  <title>Table With Colspans</title>
  <body>
    <p>Table that has colspans</p>
    <p>
      <table frame="all" id="table_fyz_ywd_gm">
        <title/>
        <tgroup cols="2">
          <colspec colname="c1" colnum="1" colwidth="1.0*"/>
          <colspec colname="c2" colnum="2" colwidth="1.0*"/>
          <tbody>
            <row>
              <entry>0:0</entry>
              <entry>0:1</entry>
            </row>
            <row>
              <entry namest="c1" nameend="c2">1:0 - 1:1</entry>
            </row>
            <row>
              <entry>2:0</entry>
              <entry>2:1</entry>
            </row>
            <row>
              <entry namest="c1" nameend="c2">3:0 - 3:1</entry>
            </row>
          </tbody>
        </tgroup>
      </table>
    </p>
    
  </body>
</topic>
