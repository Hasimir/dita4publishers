<?xml version="1.0" encoding="UTF-8"?>
<map
  class="- map/map "
  ditaarch:DITAArchVersion="1.2"
  xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"><title
    class="- topic/title ">Link Test 01</title>
  <!-- NOTE: This map also defines key key-01 but because
             it is lower in the map tree, that definition
             is not effective, even thought the map is included
             in the top-level map before the definition of 
             key-01 here.
    -->
  <mapref
    href="link_test_sub_01.ditamap"
    format="ditamap" class="+ map/topicref mapgroup-d/mapref "/>
  <topicref
    keys="key-01"
    href="topics/topic_01.xml"
    class="- map/topicref  "/><topicref
    keys="key-02"
    href="http://www.example.com"
    format="html"
    scope="external"
    class="- map/topicref "/><topicref
    keys="key-03 key-04"
    href="topics/topic_03.xml"
    class="- map/topicref "/>
  <!-- Topicref that does not define a key: -->
  <topicref
    href="topics/topic_04.xml"
    class="- map/topicref "/>
  <!-- Second declaration of key "key-01". This declaration
       is not effective because it comes later in the 
       document than the other declaration.
    -->
  <topicref
    keys="key-01"
    href="topics/topic_04.xml"
    class="- map/topicref  "/>
  <topicref
    keys="key-05"
    keyref="key-02"
    class="- map/topicref  "/>
  <keydef
    href="images/image_01.jpg"
    format="jpg" 
    class="- map/topicref  "
    keys="key-06"/></map>
