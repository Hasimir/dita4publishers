<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA Topic//EN" "topic.dtd">
<topic id="topic_jr3_l1c_3m">
  <title>Table Combo Spans Example</title>
  <body>
    <p>Table that has multiple kinds of spans
      <table id="table_xlh_csy_3m">
        <tgroup cols="5">
          <colspec colnum="1" colname="col1"/>
          <colspec colnum="2" colname="col2"/>
          <colspec colnum="3" colname="col3"/>
          <colspec colnum="4" colname="col4"/>
          <colspec colnum="5" colname="col5"/>
          <colspec colnum="6" colname="col6"/>
          <colspec colnum="7" colname="col7"/>
          <tbody>
            <row>
              <entry>0:0</entry>
              <entry>0:1</entry>
              <entry namest="col3" nameend="col5" morerows="1">
                <p>0:2 – 0:3 – 0:4</p>
                <p>1:2 – 1:3 – 1:4</p>
              </entry>
              <entry>0:5</entry>
              <entry morerows="5">
                <p>0:6</p>
                <p>1:6</p>
                <p>2:6</p>
                <p>3:6</p>
                <p>4:6</p>
                <p>5:6</p>
              </entry>
            </row>
            <row>
              <entry morerows="2">
                <p>1:0</p>
                <p>2:0</p>
                <p>3:0</p>
              </entry>
              <entry>1:1</entry>
              <entry>1:5</entry>
            </row>
            <row>
              <entry>2:1</entry>
              <entry>2:2</entry>
              <entry>2:3</entry>
              <entry>2:4</entry>
              <entry>2:5</entry>
            </row>
            <row>
              <entry>3:1</entry>
              <entry>3:2</entry>
              <entry>3:3</entry>
              <entry>3:4</entry>
              <entry>3:5</entry>
            </row>
            <row>
              <entry namest="col1" nameend="col2" morerows="1">
                <p>4:0 – 4:1</p>
                <p>5:0 – 5:1</p>
              </entry>
              <entry namest="col2" nameend="col5">
                <p>4:2 – 4:3 – 4:4 – 4:5</p>
              </entry>
            </row>
            <row>
              <entry>5:2</entry>
              <entry>5:3</entry>
              <entry>5:4</entry>
              <entry>5:5</entry>
            </row>
          </tbody>
        </tgroup>
      </table>
      
    </p>
  </body>
</topic>
